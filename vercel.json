{
  "version": 2,
  "name": "whatsapp-webhook-safari",
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    }
  ]
}